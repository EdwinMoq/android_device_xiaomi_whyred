on boot
    chown system system /sys/class/leds/lcd-backlight/max_brightness

    start vendor.light-hal-2-0

service vendor.light-hal-2-0 /vendor/bin/hw/android.hardware.light@2.0-service.whyred
    interface android.hardware.light@2.0::ILight default
    class hal
    user system
    group system
    shutdown critical
    disabled
